# Number of receipt copies
copies = 3

# Number of items on each receipt
items = 5

for copy in range(1, copies + 1):
    print(f"Receipt Copy {copy}")
    
    for item in range(1, items + 1):
        print(f"Item Number: {item}")
    
    print()  # Blank line between receipts
